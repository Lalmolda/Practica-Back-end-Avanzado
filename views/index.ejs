<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <a href="/change-locale/en">English</a>
    <a href="/change-locale/es">EspaÃ±ol</a>
    <h1><%= title %></h1>
    <p><%= __('Welcome to') %> <%= title %></p>
    <br />
    <div class="listadoContainer">
      <% anuncio.forEach(anuncio => { %>
      <div class="imageContainer">
        <p><%= __('Ad name') %> : <%= anuncio.nombre %></p>
        <p><%= __('On sale') %> : <%= anuncio.venta %></p>
        <p><%= __('Price') %> : <%= anuncio.precio %></p>
        <p>Tag: <%= anuncio.tag %></p>
        <img src="<%= anuncio.foto %>" class="picture" />
        <br />
      </div>
      <% }) %>
    </div>
  </body>
</html>
